<launch>
    <!-- Arguments -->
    <arg name="robot_namespace" default="rover_0"/>

    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization_with_gps">
        <rosparam command="load" file="$(find lidar_navigation)/config/ekf_localization.yaml" />
        <remap from="/odometry/filtered" to="/$(arg robot_namespace)/odom"/>
        <remap from="/$(arg robot_namespace)/odometry/filtered" to="/$(arg robot_namespace)/odom"/>
    </node>
</launch>